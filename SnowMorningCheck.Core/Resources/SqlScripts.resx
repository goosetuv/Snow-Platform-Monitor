  <data name="DataUpdateJobErrorLog" xml:space="preserve">
    <value>SELECT TOP 5 * FROM SnowLicenseManager.dbo.tblErrorLog
ORDER BY logDate DESC</value>
    <comment>Gets the most recent 5 error logs for DUJ</comment>
  </data>
  <data name="DataUpdateJobErrorSevere" xml:space="preserve">
    <value>SELECT TOP 5 * FROM msdb.dbo.sysjobhistory where sql_severity &gt; 0 order by run_date desc, run_time desc</value>
    <comment>Gets sysjobhistory</comment>
  </data>
  <data name="DataUpdateJobParallel" xml:space="preserve">
    <value>SELECT * FROM SnowLicenseManager.inv.tblJobParallelStep
WHERE Status = -1</value>
    <comment>Checks if parallel steps have reset as expected</comment>
  </data>
  <data name="DataUpdateJobStatus" xml:space="preserve">
    <value>SELECT
    job.name, 
    job.job_id, 
    job.originating_server, 
    activity.run_requested_date, 
    DATEDIFF( SECOND, activity.run_requested_date, GETDATE() ) as Elapsed
FROM 
    msdb.dbo.sysjobs_view job
JOIN
    msdb.dbo.sysjobactivity activity
ON 
    job.job_id = activity.job_id
JOIN
    msdb.dbo.syssessions sess
ON
    sess.session_id = activity.session_id
JOIN
(
    SELECT
        MAX( agent_start_date ) AS max_agent_start_date
    FROM
        msdb.dbo.syssessions
) sess_max
ON
    sess.agent_start_date = sess_max.max_agent_start_date
WHERE 
    run_requested_date IS NOT NULL AND stop_execution_date IS NULL AND job.name LIKE '%License Manager%'</value>
    <comment>Gets the current running status of the data update job, i.e if the job is running or not</comment>
  </data>
